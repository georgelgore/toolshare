<h1>New Reservation</h1>

<%= form_for @reservation do |f| %>
  <%=f.label :user_name %><br>
  <%=f.collection_select :user_id, User.all, :id, :name %><br><br>

  <%=f.label :item_name %><br>
  <%=f.collection_select :item_id, Item.all, :id, :name %><br><br>

  <%=f.label :start_date %><br>
  <%=f.date_select :start_date %><br><br>

  <%=f.label :end_date %><br>
  <%=f.datetime_select :end_date %><br><br>



  <%=f.submit %>
<% end %>
