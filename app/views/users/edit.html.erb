<h1>Edit User Form</h1>

<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :email %>
<%= f.text_field :email %><br>
<%= f.label :password %>
<%= f.text_field :password %><br>
<%= f.label :location %>
<%= f.text_field :location %><br>
<%= f.submit %>
<% end %>
